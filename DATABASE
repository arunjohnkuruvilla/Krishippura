Database: user
Fields:	1. user_id 					int(10)
		2. user_name 				varchar(20)
		3. user_real_name			varchar(40)
		4. user_password			varchar(30)
		5. user_creation_time		timestamp
		6. user_email_id			varchar(60)

Database: page
Fields: 1. page_id					int(10)
		2. page_category			varchar(10)
		3. page_title				varchar(100)
		4. page_creator				int(10)
		5. page_counter				int(11)
		6. page_content				text